<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">
   <View
       android:id="@+id/view"
       android:layout_width="match_parent"
       android:background="@color/bg"
       android:layout_height="@dimen/dp_10"/>

    <TextView
        android:id="@+id/tv_server_name"
        android:layout_below="@+id/view"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="27dp"
        android:layout_marginStart="27dp"
        android:layout_marginTop="10dp"
        android:text="姓名" />

    <TextView
        android:id="@+id/tv_server_phone"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignParentRight="true"
        android:layout_alignTop="@+id/tv_server_name"
        android:layout_marginEnd="20dp"
        android:layout_marginRight="20dp"
        android:text="TextView" />

    <TextView
        android:id="@+id/tv_register_time"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_server_name"
        android:layout_alignStart="@+id/tv_server_name"
        android:layout_below="@+id/tv_server_name"
        android:layout_marginTop="16dp"
        android:layout_marginBottom="@dimen/dp_10"
        android:text="注册时间" />

    <TextView
        android:id="@+id/tv_finish_num"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/tv_server_phone"
        android:layout_alignStart="@+id/tv_server_phone"
        android:layout_alignTop="@+id/tv_register_time"
        android:text="完成单数" />
</RelativeLayout>