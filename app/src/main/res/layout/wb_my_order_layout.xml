<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg"
    android:orientation="vertical"
  >
  <include layout="@layout/wb_titlebar_layout"/>

  <RelativeLayout
      android:layout_width="match_parent"
      android:layout_marginTop="@dimen/dp_10"
      android:background="@color/white"
      android:layout_height="50dp">

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:textSize="18sp"
        android:text="姓名" />

    <EditText
        android:id="@+id/et_demanderName"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerVertical="true"
        android:layout_marginLeft="87dp"
        android:layout_marginStart="87dp"
        android:layout_toEndOf="@+id/textView9"
        android:layout_toRightOf="@+id/textView9"
        android:background="@null"
        android:ems="10"
        android:hint="请输入姓名"
        android:inputType="textPersonName"
        android:textSize="@dimen/sp_14" />
  </RelativeLayout>
  <View
      android:layout_width="match_parent"
      android:layout_height="1dp"
    android:background="@color/bg"/>
  <RelativeLayout
      android:layout_width="match_parent"
      android:background="@color/white"
      android:layout_height="50dp">

    <TextView
        android:id="@+id/textView10"
        android:textSize="18sp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:text="手机号" />

    <EditText
        android:id="@+id/editText1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_centerVertical="true"
        android:layout_marginLeft="70dp"
        android:layout_marginStart="70dp"
        android:layout_toEndOf="@+id/textView10"
        android:layout_toRightOf="@+id/textView10"
        android:background="@null"
        android:ems="10"
        android:hint="请输入手机号"
        android:inputType="textPersonName"
        android:textSize="@dimen/sp_14" />
  </RelativeLayout>
  <View
      android:layout_width="match_parent"
      android:layout_height="1dp"
      android:background="@color/bg"/>
  <RelativeLayout
      android:layout_width="match_parent"
      android:background="@color/white"
      android:layout_height="50dp">

    <TextView
        android:id="@+id/textView11"
        android:layout_width="wrap_content"
        android:textSize="18sp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:text="地址" />

    <EditText
        android:id="@+id/editText2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_centerVertical="true"
        android:layout_marginLeft="88dp"
        android:layout_marginStart="88dp"
        android:layout_toEndOf="@+id/textView11"
        android:layout_toRightOf="@+id/textView11"
        android:background="@null"
        android:ems="10"
        android:hint="请输入地址"
        android:inputType="textPersonName"
        android:textSize="@dimen/sp_14" />
  </RelativeLayout>
  <View
      android:layout_width="match_parent"
      android:layout_height="1dp"
      android:background="@color/bg"/>
  <RelativeLayout
      android:layout_width="match_parent"
      android:background="@color/white"
      android:layout_height="50dp">

    <TextView
        android:id="@+id/textView12"
        android:layout_width="wrap_content"
        android:textSize="18sp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:text="身份证号" />

    <EditText
        android:id="@+id/editText3"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_centerVertical="true"
        android:layout_marginLeft="51dp"
        android:layout_marginStart="51dp"
        android:layout_toEndOf="@+id/textView12"
        android:layout_toRightOf="@+id/textView12"
        android:background="@null"
        android:ems="10"
        android:hint="请输入身份证号"
        android:inputType="textPersonName"
        android:textSize="@dimen/sp_14" />
  </RelativeLayout>
  <View
      android:layout_width="match_parent"
      android:layout_height="1dp"
      android:background="@color/bg"/>
  <RelativeLayout
      android:layout_width="match_parent"
      android:background="@color/white"
      android:layout_height="50dp">

    <TextView
        android:id="@+id/textView13"
        android:layout_width="wrap_content"
        android:textSize="18sp"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_centerVertical="true"
        android:layout_marginLeft="24dp"
        android:layout_marginStart="24dp"
        android:text="添加服务" />

    <EditText
        android:id="@+id/editText4"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_centerVertical="true"
        android:layout_marginLeft="51dp"
        android:layout_marginStart="51dp"
        android:layout_toEndOf="@+id/textView13"
        android:layout_toRightOf="@+id/textView13"
        android:background="@null"
        android:ems="10"
        android:hint="请选择"
        android:inputType="textPersonName"
        android:textSize="@dimen/sp_14" />
  </RelativeLayout>
  <com.whombang.app.common.tabGround.TagContainerLayout
      android:id="@+id/color_tag_container"
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      app:layout_marginTopPercent="2%sh"
      app:container_background_color="#00000000"
      app:container_border_color="#00000000"
      app:container_border_radius="0dp"
      app:horizontal_interval="10dp"
      app:tag_corner_radius="10dp"
      app:tag_horizontal_padding="15dp"
      app:tag_max_length="18"
      app:tag_clickable="true"
      app:tag_text_size="14sp"
      app:tag_vertical_padding="10dp"
      app:vertical_interval="10dp" />
</LinearLayout>
